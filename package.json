{
  "name": "cat-galactic",
  "version": "0.1.0",
  "description": "Chatbot avançado com inteligência artificial para automação no WhatsApp, integração via API e suporte a múltiplas funcionalidades.",
  "main": "src",
  "scripts": {
    "start": "npx pm2 start cosmic.config.js",
    "start:dev": "node ./src/auth/connection.js",
    "restart": "npx pm2 restart cosmic.config.js",
    "stop": "npx pm2 stop cosmic.config.js",
    "delete": "npx pm2 delete cosmic.config.js",
    "logs": "npx pm2 monit"
  },
