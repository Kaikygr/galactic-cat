    "dotenv": "16.4.7",
    "moment-timezone": "0.5.47",
    "node-cache": "5.1.2",
    "node-fetch": "3.3.2",
    "node-webpmux": "3.2.0",
    "pg": "^8.14.1",
    "pino": "9.6.0",
    "pm2": "^6.0.5",
    "qrcode-terminal": "0.12.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
