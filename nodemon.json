{
    "watch": [
        "src/"
    ],
    "ignore": [
        "node_modules/",
        "logs/",
        ".git/",
        "./src/auth/temp/",
        "/src/modules/geminiModule/chat_history/",
        "/src/modules/stickerModule/temp/"
    ],
    "ext": "js,json,mjs,cjs,env",
    "delay": "2500",
    "verbose": true,
    "exec": "node --trace-warnings",
    "env": {
        "NODE_ENV": "development"
    },
    "events": {
        "start": "cls || clear",
        "restart": "cls || clear"
    },
    "restartable": "rs"
}